                                </StackPanel>
                                
                            </StackPanel>
                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Column="1" Header="轴状态">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>

                            </Grid.RowDefinitions>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="F轴："></TextBlock>
                                <ToggleButton Style="{DynamicResource MetroToggleButtonSVN}" Content="调试&#13;开始" FontSize="12" IsChecked="{Binding _FRDYtoDebug}" Command="{mvvm:ActionAutoWithParam TwincatOperateAction}" CommandParameter="4"></ToggleButton>
                                <ToggleButton Style="{DynamicResource MetroToggleButtonSVN}" Content="调试&#13;复位" FontSize="12" IsChecked="{Binding _FInDebug}" Command="{mvvm:ActionAutoWithParam TwincatOperateAction}" CommandParameter="5"></ToggleButton>
                            </StackPanel>
                            <StackPanel Grid.Row="1" Orientation="Horizontal">
                                <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="F轴："></TextBlock>
                                <ToggleButton Style="{DynamicResource MetroToggleButtonSVN}" Content="SVN" FontSize="18" IsChecked="{Binding PowerOn3.Value}" Command="{mvvm:ActionAutoWithParam ServoONAction}" CommandParameter="3"></ToggleButton>
                                <Button Width="50" Height="50" Content="Rst" FontSize="18" Command="{mvvm:ActionAutoWithParam ServoResetAction}" CommandParameter="3"></Button>
                            </StackPanel>

                           

                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Row="1" Header="手动操作">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid Height="200" Width="200">
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                               
                                <Button Grid.Row="1" Content="F+" FontSize="30" PreviewMouseDown="{mvvm:ActionAuto JogActionF_Plus}" PreviewMouseUp="{mvvm:ActionAuto JogActionF_Stop}"></Button>
                                <Button Grid.Column="2" Grid.Row="1" Content="F-" FontSize="30" PreviewMouseDown="{mvvm:ActionAuto JogActionF_Minus}" PreviewMouseUp="{mvvm:ActionAuto JogActionF_Stop}"></Button>
                            </Grid>
                            <Grid Grid.Column="1" VerticalAlignment="Center">
                                <StackPanel HorizontalAlignment="Center" Orientation="Vertical">
                                    <Button Style="{StaticResource DebugButton1}" Content="上料盘" Margin="0" Command="{mvvm:ActionAutoWithParam MovetoPointAction}" CommandParameter="6"></Button>
                                    <Button Style="{StaticResource DebugButton1}" Content="穴1" Command="{mvvm:ActionAutoWithParam MovetoPointAction}" CommandParameter="7"></Button>
                                    <Button Style="{StaticResource DebugButton1}" Content="穴2" Command="{mvvm:ActionAutoWithParam MovetoPointAction}" CommandParameter="8"></Button>
                                    <Button Style="{StaticResource DebugButton1}" Content="穴3" Command="{mvvm:ActionAutoWithParam MovetoPointAction}" CommandParameter="9"></Button>
                                    <Button Style="{StaticResource DebugButton1}" Content="穴4" Command="{mvvm:ActionAutoWithParam MovetoPointAction}" CommandParameter="10"></Button>
                                    <Button Style="{StaticResource DebugButton1}" Content="NG盘" Command="{mvvm:ActionAutoWithParam MovetoPointAction}" CommandParameter="11"></Button>
                                    <!--<Button Style="{StaticResource DebugButton1}" Content="Noise盘" Command="{mvvm:ActionAutoWithParam MovetoPointAction}" CommandParameter="11"></Button>-->
                                    <Button Style="{StaticResource DebugButton1}" Content="样本盘" Command="{mvvm:ActionAutoWithParam MovetoPointAction}" CommandParameter="12"></Button>
                                </StackPanel>
                            </Grid>
                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Row="1" Grid.Column="1" Header="坐标记录">
                        <Grid VerticalAlignment="Center">
                            <StackPanel HorizontalAlignment="Center" Orientation="Vertical">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="上料盘："></TextBlock>
                                    <TextBox Style="{StaticResource TextBoxCor}" Text="{Binding FPosition1.Value}"></TextBox>
                                    
                                    <Button Style="{StaticResource DebugButton2}" Content="Get" Command="{mvvm:ActionAutoWithParam GetCoord}" CommandParameter="6"></Button>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="穴1："></TextBlock>
                                    <TextBox Style="{StaticResource TextBoxCor}" Text="{Binding FPosition2.Value}"></TextBox>
                                    
                                    <Button Style="{StaticResource DebugButton2}" Content="Get" Command="{mvvm:ActionAutoWithParam GetCoord}" CommandParameter="7"></Button>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="穴2："></TextBlock>
                                    <TextBox Style="{StaticResource TextBoxCor}" Text="{Binding FPosition3.Value}"></TextBox>
                                    
                                    <Button Style="{StaticResource DebugButton2}" Content="Get" Command="{mvvm:ActionAutoWithParam GetCoord}" CommandParameter="8"></Button>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="穴3："></TextBlock>
                                    <TextBox Style="{StaticResource TextBoxCor}" Text="{Binding FPosition4.Value}"></TextBox>
                                    
                                    <Button Style="{StaticResource DebugButton2}" Content="Get" Command="{mvvm:ActionAutoWithParam GetCoord}" CommandParameter="9"></Button>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="穴4："></TextBlock>
                                    <TextBox Style="{StaticResource TextBoxCor}" Text="{Binding FPosition5.Value}"></TextBox>
                                    
                                    <Button Style="{StaticResource DebugButton2}" Content="Get" Command="{mvvm:ActionAutoWithParam GetCoord}" CommandParameter="10"></Button>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="NG盘："></TextBlock>
                                    <TextBox Style="{StaticResource TextBoxCor}" Text="{Binding FPosition6.Value}"></TextBox>

                                    <Button Style="{StaticResource DebugButton2}" Content="Get" Command="{mvvm:ActionAutoWithParam GetCoord}" CommandParameter="11"></Button>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="样本盘："></TextBlock>
                                    <TextBox Style="{StaticResource TextBoxCor}" Text="{Binding FPosition7.Value}"></TextBox>

                                    <Button Style="{StaticResource DebugButton2}" Content="Get" Command="{mvvm:ActionAutoWithParam GetCoord}" CommandParameter="12"></Button>
                                </StackPanel>
                                <!--<StackPanel Orientation="Horizontal">
                                    <TextBlock Style="{StaticResource  TextBlockParameterName}" Text="Noise盘："></TextBlock>
                                    <TextBox Style="{StaticResource TextBoxCor}" Text="{Binding FPosition8.Value}"></TextBox>

                                    <Button Style="{StaticResource DebugButton2}" Content="Get" Command="{mvvm:ActionAutoWithParam GetCoord}" CommandParameter="24"></Button>
                                </StackPanel>-->
                            </StackPanel>
                        </Grid>
                    </GroupBox>
                </Grid>
            </TabItem>
            <TabItem Header="运输">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition></RowDefinition>
                        <RowDefinition Height="1.5*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
